{
  // Serilog
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "Jwt": {
    "ValidForDays": "1",
    "SecretKey": "string"
  },
  "ConnectionStrings": {
    "Primary": "Server=(localdb)\\mssqllocaldb;Database=string;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "BasicAuthentication": {
    "UserName": "string",
    "Password": "string"
  },
  "AzureAdB2CApi": {
    "ClientId": "00000000-0000-0000-0000-000000000000",
    "Instance": "https://string.b2clogin.com",
    "SignUpSignInPolicyId": "string",
    "Domain": "string.onmicrosoft.com",
    "SignedOutCallbackPath": "/signout/string",
    "ResetPasswordPolicyId": "string",
    "EditProfilePolicyId": "string",
    "CallbackPath": "/signin-oidc"
  },
  "AzureAdB2CSwagger": {
    "ClientId": "00000000-0000-0000-0000-000000000000",
    "ClientSecret": "string",
    "AppName": "string",
    "Scope": "https://string.onmicrosoft.com/00000000-0000-0000-0000-000000000000/API.Access"
  },
  "AzureAdB2CManagement": {
    "ClientId": "00000000-0000-0000-0000-000000000000",
    "ClientSecret": "string",
    "TenantId": "00000000-0000-0000-0000-000000000000"
  },
  "Graph": {
    "BaseUrl": "https://graph.microsoft.com/v1.0",
    "Scopes": [ "https://graph.microsoft.com/.default" ]
  },
  "AzureStorage": {
    "ConnectionString": "string",
    "UserProfileImageContainer": "string"
  },
  "Mail": {
    "SendGridSMTP": {
      "FromName": "string",
      "FromEmail": "string@gmail.com",
      "ApiKey": "string"
    },
    "GoogleSMTP": {
      "FromName": "string",
      "FromEmail": "string@gmail.com",
      "NetworkCredUsername": "string@gmail.com",
      "HostName": "smtp.gmail.com",
      "NetworkCredPassword": "string",
      "Port": 587,
      "EnableSSL": true
    }
  },
  "CORS": {
    "Blazor": [ "string" ]
  },
  "Cache": {
    "UseRedis": false,
    "UseDistributedCache": false,
    "DefaultAbsoluteExpirationSeconds": 3600,
    "DefaultSlidingExpirationSeconds": 60,
    "Redis": {
      "RedisURL": "string"
    }
  },
  "AllowedHosts": "*"
}